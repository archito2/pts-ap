{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.dolphin-pts",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "100.0.9"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"apService": {
				"uri": "proxy_abap/sap/opu/odata/dol/AP_GW_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.dolphin.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"data": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/localTiles.json",
				"preload": true
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.dolphin.i18n.i18n"
				}
			},
			"apModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultCountMode": "None",
					"useBatch": false
				},
				"dataSource": "apService"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.dolphin.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "home",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "",
					"name": "hitlist",
					"target": "hitlist"
				},
				{
					"pattern": "home",
					"name": "home",
					"target": [
						"home"
					]
				},
				{
					"pattern": "apDetail/{RecNo}",
					"name": "apDetail",
					"target": "apDetail"
				},
				{
					"pattern": "createER",
					"name": "createER",
					"target": "createER"
				}
			],
			"targets": {
				"hitlist": {
					"viewName": "Hitlist",
					"viewLevel": 1
				},
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"apDetail": {
					"viewName": "Detail",
					"viewLevel": 2
				},
				"createER": {
					"viewName": "ERDetail",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}